{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug SequelFast",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/SequelFast",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "QT_QPA_PLATFORM_PLUGIN_PATH",
                    "value": "${workspaceFolder}/Qt-link/6.9.1/gcc_64/plugins"
                },
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/Qt-link/6.9.1/gcc_64/lib:${env:LD_LIBRARY_PATH}"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "engineLogging": false,
                "programOutput": true
            }
        },
        {
            "name": "Launch SequelFast (No Debug)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/SequelFast",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "QT_QPA_PLATFORM_PLUGIN_PATH",
                    "value": "${workspaceFolder}/Qt-link/6.9.1/gcc_64/plugins"
                },
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/Qt-link/6.9.1/gcc_64/lib:${env:LD_LIBRARY_PATH}"
                }
            ],
            "externalConsole": false,
            "preLaunchTask": "build"
        }
    ]
}